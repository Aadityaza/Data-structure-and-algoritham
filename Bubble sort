#include<stdio.h>


void swap(int *a,int*b){
    int temp=*a;
    *a=*b;
    *b=temp;
}
void Bub_sort(int *arr,int n){


       for(int j=0;j!=n;j++){
           int count=0;
           for(int i=0;(i+1)!=n;i++){
                if(arr[i]>arr[i+1]){
                    count++;
                    swap(&arr[i+1],&arr[i]);
                }
        }
        if(count==0)
           break;

   }

}
void disp(int *a ,int n){
    for(int i=0 ; i!=n;i++){
        printf("%d ",a[i]);
    }
}
int main()
{
    int arr[]={9,8,7,6,5,4,3,2};
    Bub_sort(arr,7);
    disp(arr,7);
}
